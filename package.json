    "is-numeric": "0.0.9",
    "is-string": "^1.1.1",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.14.1",
    "pako": "^2.1.0",
    "param-case": "^3.0.4",
    "pkg.json": "^2.0.8",
    "preact": "^10.25.4",
    "react": "npm:@preact/compat@^18.3.1",
    "react-dom": "npm:@preact/compat@^18.3.1",
    "screenfull": "^6.0.2",
    "simple-color-picker": "^1.0.5",
    "tinycolor2": "^1.6.0",
    "tweakpane": "^4.0.5",
    "uuid": "^9.0.1",
